generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Persona {  
  id        Int      @id @default(autoincrement()) 
  nombre    String
  apellido  String
  direccion String
  telefono  String
  municipio String
  creadoEn  DateTime @default(now())
}
